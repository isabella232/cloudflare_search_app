{
  "name": "Swiftype Search",
  "description": "Add search to your website with one click, then see what your visitors are searching for on your site.",
  "version": "0.8",
  "category": "site search",
  "main": "public/javascripts/swiftype.js",
  "config": {
    "details":{
      "Language": "English",
      "Price": "Free",
      "Restrictions": "Indexes up to 1,000 pages on your domain.",
      "TOS": "https://swiftype.com/terms",
      "has_js": true
    },
    "assets":{
      "logos" : {
        "200px": "./public/images/logo-200.png",
        "132px": "./public/images/logo-132.png"
      },
      "detail_page": [
        "./public/images/search-overlay.png"
      ]
    },
    "interface": [
      {
        "type": "hidden",
        "id": "engine_key",
        "name": "[Swiftype engine key]",
        "value": "missing",
        "domain_request": true
      }
    ]
  },
  "account": {
    "callback_url": "https://swiftype.com/cloudflare",
    "user_fields": ["email"]
  }
}
